{
  "name": "openfin-notifications",
  "version": "1.9.1",
  "engines": {
    "node": ">=12"
  },
  "description": "Client library for the Openfin Notifications Center",
  "main": "dist/client/main-bundle.js",
  "types": "dist/client/index.d.ts",
  "files": [
    "dist/client"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "build": "svc-tools build --mode production",
    "build:dev": "svc-tools build --mode development",
    "channels": "svc-tools channels",
    "clean": "rimraf gen dist",
    "docs": "typedoc --tsconfig \"./src/client/tsconfig.json\"",
    "fix": "svc-tools fix",
    "posttest": "npm run check",
    "prepack": "tsc -p src/client/tsconfig.types.json",
    "lint": "npm run lint:typescript",
    "lint:typescript": "npx eslint \"./src/**/*.{ts,tsx}\"",
    "lint:styles": "npx stylelint \"./src/**/*.{ts,tsx}\"",
    "test": "npm run test:unit && npm run test:int",
    "test:int": "svc-tools test int",
    "test:unit": "jest",
    "zip": "svc-tools zip",
    "start": "svc-tools start",
    "git:pre-push": "npm run lint && npm run test:unit",
    "prepare": "husky install"
  },
  "author": "Openfin",
  "license": "ISC",
  "homepage": "https://developers.openfin.co/docs/notifications-api",
  "devDependencies": {
    "@openfin/predux": "^1.0.0-alpha.1",
    "@openfin/ui-library": "^0.1.25",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "6.2.9",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^24.8.0",
    "@types/jsdom": "^11.0.4",
    "@types/lodash": "^4.14.168",
    "@types/markdown-it": "0.0.8",
    "@types/mkdirp": "^0.5.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.4.6",
    "@types/node-fetch": "^2.3.4",
    "@types/openfin": "^51.0.0",
    "@types/puppeteer-core": "^2.0.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-redux": "^7.0.8",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-transition-group": "^4.2.4",
    "@types/react-virtualized": "^9.21.11",
    "@types/semver-compare": "^1.0.0",
    "@types/styled-components": "^5.1.7",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.10",
    "babel-preset-const-enum": "^1.0.0",
    "dexie": "^2.0.4",
    "eslint": "^7.25.0",
    "eslint-plugin-jest": "^24.3.5",
    "express": "^4.16.2",
    "express-http-proxy": "^1.6.0",
    "fake-indexeddb": "^2.1.1",
    "fetch-mock": "^9.2.1",
    "focus-visible": "^5.1.0",
    "formik": "^2.2.5",
    "framer-motion": "^4.1.5",
    "fuse.js": "^6.4.6",
    "hadouken-js-adapter": "^1.44.1",
    "history": "^4.10.1",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^6.0.0",
    "inversify": "^5.0.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-dom": "^4.0.0",
    "jsdom": "^15.1.1",
    "lodash": "^4.17.21",
    "markdown-it": "^9.1.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.1",
    "node-fetch": "^2.6.0",
    "openfin-service-async": "^1.0.1",
    "openfin-service-config": "^1.0.4",
    "openfin-service-signal": "^1.0.0",
    "openfin-service-tooling": "^1.0.42-alpha.29",
    "prettier": "^2.2.1",
    "puppeteer-core": "~5.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.2.2",
    "react-virtualized": "^9.22.3",
    "redux": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "reselect": "^4.0.0",
    "semver-compare": "^1.0.0",
    "styled-components": "^5.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2",
    "workbox-webpack-plugin": "^5.1.4",
    "yup": "^0.32.9"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:jest/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "project": [
        "./tsconfig.json",
        "./src/**/tsconfig.json",
        "./test/**/tsconfig.json"
      ],
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "jest"
    ],
    "ignorePatterns": [
      "**/*.spec.tsx",
      "**/*.stories.tsx"
    ],
    "rules": {
      "dot-notation": "off",
      "@typescript-eslint/dot-notation": "error",
      "react/prop-types": "off",
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/explicit-member-accessibility": [
        "error",
        {
          "overrides": {
            "constructors": "off"
          }
        }
      ],
      "@typescript-eslint/no-unused-vars": "warn",
      "@typescript-eslint/no-explicit-any": "warn"
    },
    "globals": {
      "fin": "readonly",
      "JSX": "readonly"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "node": true
    }
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ]
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": true,
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "arrowParens": "always",
    "printWidth": 100
  }
}
